                    SettingsItems["CloseButton"]:OnHoverLeave(function()
                        SettingsItems["Text"]:ChangeItemTheme({TextColor3 = "Dark Text"})
                        SettingsItems["Text"]:Tween(nil, {TextColor3 = Library.Theme["Dark Text"]})
